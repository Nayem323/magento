<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="product.info.main">
            <block class="Magento\Catalog\Block\Product\View\Description" name="product.info.videoplayer" template="N23_Videoplayer::product/view/attribute.phtml" after="product.info.overview" ifconfig="n23_videoplayer/general/enable">
                <arguments>
                    <argument name="at_call" xsi:type="string">getVideoplayer</argument>
                    <argument name="at_code" xsi:type="string">videoplayer</argument>
                    <argument name="css_class" xsi:type="string">videoplayer</argument>
                    <argument name="at_label" xsi:type="string">none</argument>
                    <argument name="at_type" xsi:type="string">image</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
